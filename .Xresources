xterm*foreground:white
xterm*background:#222222

xterm*font: -*-terminus-medium-*-*-*-12-*-*-*-*-*-*-*
xterm*boldFont: -*-terminus-bold-*-*-*-12-*-*-*-*-*-*-*

!xterm*font: -*-terminus-medium-*-*-*-14-*-*-*-*-*-*-*
!xterm*boldFont: -*-terminus-bold-*-*-*-14-*-*-*-*-*-*-*

!xterm*font: -*-proggycleanttsz-*-*-*-*-0-*-*-*-*-*-*-*

!xterm*font: -*-orp-medium-r-semicondensed-*-13-*-*-*-*-*-*-*
!xterm*font: -*-profont-*-*-*-*-*-*-*-*-*-*-*-*
!xterm*font: -*-dina-medium-r-normal-*-13-80-*-*-*-*-*-*
xterm*VT100.geometry:     160x60
xterm*utf8:              1
xterm*VT100*eightBitInput:     true
xterm*VT100*eightBitControl:   false
xterm*VT100*eightBitOutput:    true
xterm*metaSendsEscape:    false
xterm*altSendsEscape:     false
xterm*scrollTtyOutput:    false
xterm*jumpScroll:         true
xterm*multiScroll:        true
xterm*scrollBar:          false
xterm*rightScrollBar:     false
xterm*allowBoldFonts:     true
xterm*wideBoldFont:       true
xterm*boldMode:           false
xterm*saveLines:          1000
xterm*cursorColor:        green
xterm*appkeypadDefault:   true
xterm*charClass:          33:48,35:48,37:48,43:48,45-47:48,64:48,95:48,126:48
xterm*selectToClipboard:  true

!xterm*VT100.Translations: #override \
!    Meta<KeyPress> d:    string("ciao")\n\
!    <Key>Super_L:  string("\033\033[24~")
!Meta<Key>d:    string(0x1b) string("d")


st*foreground:white
st*background:#222222
st*font: -*-terminus-*-*-*-*-*-*-*-*-*-*-*-*
!st*font: -*-orp-medium-r-semicondensed-*-13-*-*-*-*-*-*-*
!st*font: -*-profont-*-*-*-*-*-*-*-*-*-*-*-*
!st*font:              -*-dina-medium-r-normal-*-13-80-*-*-*-*-*-*
st*VT100.geometry:     160x60
st*utf8:              1
st*VT100*eightBitInput:     true
st*VT100*eightBitControl:   false
st*VT100*eightBitOutput:    true
st*metaSendsEscape:    false
st*altSendsEscape:     false
st*scrollTtyOutput:    false
st*jumpScroll:         true
st*multiScroll:        true
st*scrollBar:          false
st*rightScrollBar:     false
st*boldMode:           false
st*saveLines:          1000
st*cursorColor:        green
st*appkeypadDefault:   true
st*charClass:          33:48,35:48,37:48,43:48,45-47:48,64:48,95:48,126:48
st*selectToClipboard:  true

!st*VT100.Translations: #override \
!    Meta<KeyPress> d:    string("ciao")\n\
!    <Key>Super_L:  string("\033\033[24~")
!Meta<Key>d:    string(0x1b) string("d")

